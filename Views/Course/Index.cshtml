@model CourseStudentsViewModel

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Course</title>
</head>
<body>
    Course/index View
    
    <h2>@ViewBag.count</h2>

    <h2>
        @(
            ((Course)ViewBag.course).Name
        )
    </h2>

    <h1>
        @(
    ((Course)ViewData["course"]).Name
        )
    </h1>

@*@Model.Name*@ 

<h1>@Model.Course.Name</h1> 
<ul>
    @foreach (var item in Model.Students)
    {
        <li>
            @item.Name
        </li>
    }
</ul>

</body>
</html>